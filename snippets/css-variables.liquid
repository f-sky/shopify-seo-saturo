h1, h2, h3, h4, h5, p, a {
  margin-top: 0;
  margin-bottom: 0;
  color: var(--color-black-grey-title);
}

a {
  text-decoration: none;
  color: var(--color-turquoise);
}

a[href*="#references"] {
  vertical-align: super;
  font-size: calc(var(--font-size-a-references) * 0.1rem);
}

.h00 {
  font-size: calc(var(--font-size-h00-desktop) * 0.1rem);
  font-weight: var(--font-weight-h00);
  line-height: var(--default-line-height);
}

h1,
.h1-headline {
  font-size: calc(var(--font-size-h1-headline-desktop) * 0.1rem);
  font-weight: var(--font-weight-h1-headline);
  line-height: var(--line-height-h1-headline);
  font-family: var(--font-family-two);
}

h2,
.h2-headline {
  font-size: calc(var(--font-size-h2-headline-desktop) * 0.1rem);
  font-weight: var(--font-weight-h2-headline);
  line-height: var(--default-line-height);
  letter-spacing: 0;
}
  
.h2-headline.bold {
  font-weight: bold;
}

h5,
.h5-headline {
  font-size: calc(var(--font-size-h5-headline-desktop) * 0.1rem);
  font-weight: var(--font-weight-h5-headline);
  line-height: var(--default-line-height);
}

h6,
.h6-headline {
  font-size: calc(var(--font-size-h6-headline-desktop) * 0.1rem);
  font-weight: var(--font-weight-h6-headline);
  line-height: var(--default-line-height);
}

p,
.p {
  font-size: calc(var(--font-size-p-desktop) * 0.1rem);
  font-weight: var(--font-weight-p);
  line-height: var(--default-line-height);
  margin-bottom: 1.5rem;
}

.p-italic {
  font-size: calc(var(--font-size-p-desktop) * 0.1rem);
  font-weight: var(--font-weight-p);
  line-height: var(--default-line-height);
  font-style: italic;
  margin-bottom: 1.5rem;
}

.p-link {
  font-size: calc(var(--font-size-p-desktop) * 0.1rem);
  font-weight: var(--font-weight-p);
  line-height: var(--default-line-height);
  color: var(--color-link);
  text-decoration: none;
  margin-bottom: 1.5rem;
}

.details {
  font-size: calc(var(--font-size-details-desktop) * 0.1rem);
  font-weight: var(--font-weight-details);
  line-height: var(--default-line-height);
}

.menu {
  font-size: calc(var(--font-size-menu-desktop) * 0.1rem);
  font-weight: var(--font-weight-menu);
  line-height: var(--default-line-height);
  text-decoration: none;
}

.submenu {
  font-size: calc(var(--font-size-submenu-desktop) * 0.1rem);
  font-weight: var(--font-weight-submenu);
  line-height: var(--default-line-height);
  text-decoration: none;
}

.navigation {
  font-size: calc(var(--font-size-navigation-desktop) * 0.1rem);
  font-weight: var(--font-weight-navigation);
  line-height: var(--default-line-height);
}

.input-wrapper {
  position: relative;
  display: flex;
}

.input-wrapper .input {
  appearance: none;
  -webkit-appearance: none;
  border: none;
  border-radius: 2.3rem;
  padding: 1.3rem 2rem;
  height: 4.6rem;
  font-size: calc(var(--font-size-input-desktop) * .1rem);
  line-height: calc(var(--font-size-input-desktop) * .1rem);
  font-weight: var(--font-weight-input);
  font-family: var(--font-body-family);
  box-shadow: 0 0 0 .1rem var(--color-dark-light-2);
  color: var(--color-grey-title);
  background-color: transparent;
  outline: none;
  width: 100%;
}

.input-wrapper .radio-hidden {
  position: absolute;
  opacity: 0;
  width: 0.1rem;
  height: 0.1rem;
}

.input-wrapper .select {
  border: none;
  border-radius: 2.3rem;
  padding: 1rem 4rem 1rem 2rem;
  height: 4.6rem;
  font-size: calc(var(--font-size-select-desktop) * .1rem);
  line-height: calc(var(--font-size-select-desktop) * .1rem);
  font-weight: var(--font-weight-select);
  font-family: var(--font-body-family);
  box-shadow: 0 0 0 .1rem var(--color-dark-light-2);
  color: var(--color-grey-title);
  background-color: transparent;
  outline: none;
  width: 100%;
  -webkit-appearance: none;
  appearance: none;
}

.input-wrapper .select ~ .label {
  pointer-events: none;
}


.input-wrapper .select:not(.has-option) ~ .label {
  font-size: 1.8rem;
  font-weight: bold;
}

.input-wrapper .select:disabled,
.input-wrapper .select:disabled + .label,
.input-wrapper .select:disabled ~ .select-chevron {
  opacity: 0.4;
}

.input-wrapper .select-chevron {
  position: absolute;
  right: 1.5rem;
  top: 1.1rem;
  pointer-events: none;
}

.input-wrapper .select-chevron svg {
  transform: rotate(90deg);
  width: 1.6rem;
}

.input-wrapper .select-chevron svg path {
  fill: var(--color-black-grey-title)
}

.input-wrapper .select::placeholder,
.input-wrapper .input::placeholder {
  opacity: 0;
}
.input-wrapper textarea.input::placeholder {
  opacity: 1;
  color: var(--color-black-grey-title);
}

.input-wrapper .select:hover,
.input-wrapper .input:hover {
  box-shadow: 0 0 0 .1rem var(--color-grey-title);
}

.input-wrapper .label {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 2rem;
  color: var(--color-grey-title);
  font-size: calc(var(--font-size-input-desktop) * .1rem);
  line-height: calc(var(--font-size-input-desktop) * .1rem);
  font-weight: var(--font-weight-p);
  font-family: var(--font-body-family);
  transition: all 0.2s ease-in-out;
}

.input-wrapper .select:focus ~ .label,
.input-wrapper .select.has-option ~ .label,
.input-wrapper .select:-webkit-autofill ~ .label,
.input-wrapper .input:focus ~ .label,
.input-wrapper .input:not(:placeholder-shown) ~ .label,
.input-wrapper .input:-webkit-autofill ~ .label {
  top: 0rem;
  transform: none;
  font-size: 1.3rem;
  line-height: 1.3rem;
}

/* .input-wrapper .select:not(:placeholder-shown), */
.input-wrapper .select.has-option,
.input-wrapper .select:focus,
.input-wrapper .select:-webkit-autofill,
.input-wrapper .input:focus,
.input-wrapper .input:not(:placeholder-shown),
.input-wrapper .input:-webkit-autofill {
  box-shadow: 0 0 0 .2rem var(--color-turquoise);
}

.input-wrapper .select:focus,
.input-wrapper .input:focus {
  outline-offset: 0.2rem;
  outline: 4px solid var(--color-turquoise-light);
}

.input-wrapper .select-light,
.input-wrapper .input-light {
  color: var(--color-white);
  box-shadow: 0 0 0 .1rem var(--color-white);
}

.input-wrapper .input-light:hover {
  box-shadow: 0 0 0 .1rem var(--color-white);
}

.input-wrapper .label-light {
