{% comment %}
  This is used to create CSS custom variables with Liquid references
  The file variables.scss is referring these CSS custom variables as Sass variables
  to use throughout the stylesheet 
{% endcomment %}
{% style %}
  {%- assign header_font = settings.type_header_font -%}
  {%- assign h0_font = settings.type_h0_font -%}
  {%- assign h2_font = settings.type_h2_font -%}
  {%- assign h3_font = settings.type_h3_font -%}
  {%- assign h4_font = settings.type_h4_font -%}
  {%- assign h5_font = settings.type_h5_font -%}
  {%- assign h6_font = settings.type_h6_font -%}
  {%- assign announcementbar_font = settings.type_announcementbar_font -%}
  {%- assign base_font = settings.type_base_font -%}
  {%- assign base_font_bolder = base_font | font_modify: 'weight', 'bolder' -%}
  {%- assign base_font_bold = base_font | font_modify: 'weight', 'bold' -%}
  {%- assign base_font_italic = base_font | font_modify: 'style', 'italic' -%}
  {%- assign base_font_bold_italic = base_font_bold | font_modify: 'style', 'italic' -%}

  :root {
    --color-text: {{ settings.color_text }};
    --color-text-rgb: {{ settings.color_text | color_to_rgb | remove: 'rgb(' | remove: 'rgba(' | remove: ')' | remove: ', 0.0' }};
    --color-body-text: {{ settings.color_body_text }};
    --color-sale-text: {{ settings.color_sale_text }};
    --color-feature-text: {{ settings.color_feature_text }};
    --color-small-button-text-border: {{ settings.color_small_button_text_border }};
    --color-text-field: {{ settings.color_text_field }};
    --color-text-field-text: {{ settings.color_text_field_text }};
    --color-text-field-text-rgb: {{ settings.color_text_field_text | color_to_rgb | remove: 'rgb(' | remove: 'rgba(' | remove: ')' | remove: ', 0.0' }};
    
    --color-text-highlight-1: {{ settings.color_highlight_1 }};
    --color-text-light: {{ settings.color_light }};

    --color-btn-primary: {{ settings.color_button }};
    --color-btn-primary-darker: {{ settings.color_button | color_darken: 10 }};
    --color-btn-primary-text: {{ settings.color_button_text }};

    --color-btn-secondary: {{ settings.color_button_secondary }};
    --color-btn-secondary-text: {{ settings.color_button_secondary_text }};

    --color-btn-tertiary: {{ settings.color_button_tertiary }};
    --color-btn-tertiary-text: {{ settings.color_button_tertiary_text }};

    --color-btn-white-accent-text: {{ settings.color_button_white_accent_text }};
    

    --color-btn-highlight: {{ settings.color_button_highlight }};

    --color-blankstate: {{ settings.color_body_text | color_modify: 'alpha', 0.35 }};
    --color-blankstate-border: {{ settings.color_body_text | color_modify: 'alpha', 0.2 }};
    --color-blankstate-background: {{ settings.color_body_text | color_modify: 'alpha', 0.1 }};

    --color-text-focus: {%- include 'adaptive-color' with settings.color_text as color -%};
    --color-text-link: {{ settings.color_text_link }};
    --color-overlay-text-focus: {%- include 'adaptive-color' with settings.color_image_overlay_text as color -%};
    --color-btn-primary-focus: {%- include 'adaptive-color' with settings.color_button as color -%};
    --color-btn-highlight-focus: {%- include 'adaptive-color' with settings.color_button_highlight as color -%};
    --color-btn-social-focus: {%- include 'adaptive-color' with settings.color_borders as color -%};
    --color-small-button-text-border-focus: {%- include 'adaptive-color' with settings.color_small_button_text_border as color -%};
    --predictive-search-focus: {%- include 'adaptive-color' with settings.color_body_bg as color, is_background_color: true -%};

    --color-body: {{ settings.color_body_bg }};
    --color-bg: {{ settings.color_body_bg }};
    --color-bg-rgb: {{ settings.color_body_bg | color_to_rgb | remove: 'rgb(' | remove: 'rgba(' | remove: ')' | remove: ', 0.0' }};
    --color-bg-alt: {{ settings.color_body_text | color_modify: 'alpha', 0.05 }};
    --color-bg-currency-selector: {{ settings.color_body_text | color_modify: 'alpha', 0.2 }};

    --color-overlay-title-text: {{ settings.color_image_overlay_text }};
    --color-image-overlay: {{ settings.color_image_overlay }};
    --color-image-overlay-rgb: {{ settings.color_image_overlay | color_to_rgb | remove: 'rgb(' | remove: 'rgba(' | remove: ')' | remove: ', 0.0' }};

    --color-review-star-highlight: {{ settings.color_review_star_highlight }};
    --color-review-star-dark: {{ settings.color_review_star_dark }};
    --size-review-star-small: {{ settings.size_review_star_small }};


    {%- assign image_overlay_opacity = settings.image_overlay_opacity | divided_by: 100.00 -%}
    --opacity-image-overlay: {{ image_overlay_opacity }};

    {%- comment -%}
      Based on the image overlay opacity, either lighten or darken the image on hover
    {%- endcomment -%}
    {%- if image_overlay_opacity > 0.85 -%}
      {%- assign image_overlay_opacity_hover = image_overlay_opacity | minus: 0.3 -%}
    {%- else -%}
      {%- assign image_overlay_opacity_hover = image_overlay_opacity | plus: 0.4 -%}
    {%- endif -%}
    --hover-overlay-opacity: {{ image_overlay_opacity_hover | at_most: 1 }};

    --color-border: {{ settings.color_borders }};
    --color-border-form: {{ settings.color_text_field_border }};
    --color-border-form-darker: {{ settings.color_text_field_border | color_darken: 10}};
    --color-border-form-darkest: {{ settings.color_text_field_border | color_darken: 100}};


    --svg-select-icon: url({{ "ico-select.svg" | asset_url }});
    --slick-img-url: url({{ "ajax-loader.gif" | asset_url }});

    --font-weight-body--bold: {{ base_font_bold.weight | default: 700 }};
    --font-weight-body--bolder: {{ base_font_bolder.weight | default: 700 }};

    --font-stack-header: {{ header_font.family }}, {{ header_font.fallback_families }};
    --font-style-header: {{ header_font.style }};
    --font-weight-header: {{ header_font.weight | default: 600 }};
    --font-size-header: {{ settings.type_header_base_size }};
    --font-size-header-mobile: {{ settings.type_header_mobile_size }};

    --font-stack-h0: {{ h0_font.family }}, {{ header_font.fallback_families }};
    --font-style-h0: {{ h0_font.style }};
    --font-weight-h0: {{ h0_font.weight | default: 600 }};
    --font-size-h0-desktop: {{ settings.type_h0_desktop }};
    --font-size-h0-mobile: {{ settings.type_h0_mobile }};

    --font-stack-h2: {{ h2_font.family }}, {{ header_font.fallback_families }};
    --font-style-h2: {{ h2_font.style }};
    --font-weight-h2: {{ h2_font.weight | default: 600 }};
    --font-size-h2-desktop: {{ settings.type_h2_desktop }};
    --font-size-h2-mobile: {{ settings.type_h2_mobile }};

    --font-stack-h3: {{ h3_font.family }}, {{ header_font.fallback_families }};
    --font-style-h3: {{ h3_font.style }};
    --font-weight-h3: {{ h3_font.weight | default: 600 }};
    --font-size-h3-desktop: {{ settings.type_h3_desktop }};
    --font-size-h3-mobile: {{ settings.type_h3_mobile }};

    --font-stack-h4: {{ h4_font.family }}, {{ header_font.fallback_families }};
    --font-style-h4: {{ h4_font.style }};
    --font-weight-h4: {{ h4_font.weight | default: normal }};
    --font-size-h4-desktop: {{ settings.type_h4_desktop }};
    --font-size-h4-mobile: {{ settings.type_h4_mobile }};

    --font-stack-h5: {{ h5_font.family }}, {{ header_font.fallback_families }};
    --font-style-h5: {{ h5_font.style }};
    --font-weight-h5: {{ h5_font.weight | default: 600 }};
    --font-size-h5-desktop: {{ settings.type_h5_desktop }};
    --font-size-h5-mobile: {{ settings.type_h5_mobile }};

    --font-stack-h6: {{ h6_font.family }}, {{ header_font.fallback_families }};
    --font-style-h6: {{ h6_font.style }};
    --font-weight-h6: {{ h6_font.weight | default: normal }};
    --font-size-h6-desktop: {{ settings.type_h6_desktop }};
    --font-size-h6-mobile: {{ settings.type_h6_mobile }};

    --font-stack-announcementbar: {{ announcementbar_font.family }}, {{ header_font.fallback_families }};
    --font-style-announcementbar: {{ announcementbar_font.style }};
    --font-weight-announcementbar: {{ announcementbar_font.weight | default: normal }};
    --font-size-announcementbar-desktop: {{ settings.type_announcementbar_desktop }};

    --font-stack-body: {{ base_font.family }}, {{ base_font.fallback_families }};
    --font-style-body: {{ base_font.style }};
    --font-weight-body: {{ base_font.weight }};

    --font-size-base: {{ settings.type_base_size }};     

    --font-mega-title-large-desktop: {{ settings.type_header_base_size | times: 2.5 | floor }};

    --font-rich-text-large: {{ settings.type_base_size | times: 1.15 | floor }};
    --font-rich-text-small: {{ settings.type_base_size | times: 0.88 | floor }};

    --font-size-text-small: {{ settings.type_base_size | times: 0.88 | floor }};
    --font-size-text-large: {{ settings.type_base_size | times: 1.15 | floor }};

    {% comment %} Create an appropriate poster background color based on background color and brightness {% endcomment %}
    {%- assign color_body_bg = settings.color_body_bg -%}
    {%- assign color_body_bg_brightness = color_body_bg | color_brightness -%}
    {%- if color_body_bg_brightness <= 26 -%}
      {%- assign background_color_poster = color_body_bg | color_lighten: 10 -%}
    {%- elsif color_body_bg_brightness <= 65 -%}
      {%- assign background_color_poster = color_body_bg | color_lighten: 5 -%}
    {%- else -%}
      {%- assign background_color_poster = color_body_bg | color_darken: 5 -%}
    {%- endif -%}

    --color-video-bg: {{ background_color_poster }};

    {% comment %} Image placeholder {% endcomment %}
    --global-color-image-loader-primary: {{ settings.color_text | color_modify: 'alpha', 0.06 }};
    --global-color-image-loader-secondary: {{ settings.color_text | color_modify: 'alpha', 0.12 }};
  }

